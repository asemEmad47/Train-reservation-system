  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT TRIP.DESTINATION, SUM(TRIP.PRICE * (TRAIN.NUMOFSEATS - TRAIN.AVAILABLESEATS)) AS totalRevenue
FROM TRIP
INNER JOIN TRAIN ON TRIP.TRAINID = TRAIN.trainid
GROUP BY TRIP.DESTINATION;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DESTINATION">
