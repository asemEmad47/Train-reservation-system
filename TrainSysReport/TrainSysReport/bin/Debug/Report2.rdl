  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT MAX(totalRevenue) AS MaxRevenue
FROM (
    SELECT TRIP.TRIPID, TRIP.ORIGIN, TRIP.DESTINATION, TRAIN.NUMOFSEATS - TRAIN.AVAILABLESEATS AS NumOfBookings, TRIP.PRICE * (TRAIN.NUMOFSEATS - TRAIN.AVAILABLESEATS) AS totalRevenue
    FROM TRIP
    INNER JOIN TRAIN ON TRIP.TRAINID = TRAIN.trainid
) AS subquery;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
